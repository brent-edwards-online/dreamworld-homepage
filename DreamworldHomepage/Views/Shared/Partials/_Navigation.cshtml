@model DreamworldHomepage.ViewModels.NavigationViewModel

<style>
    #main-navigation {
        z-index: 10;
        position: fixed;
    }

    #main-navigation > ul {
        list-style: none;
    }

    #main-navigation > ul > li {
        float: left;
        text-decoration: none;
        padding: 0 10px;
    }

    #main-navigation > ul > li > a {
        color: #ffffff;
        font-family: 'Amsi Pro';
    }
</style>

<nav id="main-navigation">
    @{
        <ul>
            @foreach (var item in Model.MenuItems)
            {
                <li><a href="">@item.Text</a></li>
            }
        </ul>
    }
</nav>



